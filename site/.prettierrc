{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 120,
	"plugins": [
		"./node_modules/prettier-plugin-svelte/plugin.js",
		"./node_modules/prettier-plugin-tailwindcss/dist/index.mjs"
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"tailwindStylesheet": "./src/app.css"
}
